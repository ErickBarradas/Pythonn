idades = []
alturas = []
for i in range(1, 6):
    print('%dÂº Pessoa' %i)
    idade = int(input('Digite idade: '))
    altura = float(input('Digite altura: '))
    idades.append(idade)
    alturas.append(altura)
print('ordem contrÃ¡ria')
print('altura')
print(alturas[::-1])
print('idade')
print(idades[::-1])
print('ordem correta')
print('altura')
print(alturas)
print('idade')
print(idades)

#Criadas duas listas e usado o for com range
